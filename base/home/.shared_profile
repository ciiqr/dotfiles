#!/usr/bin/env bash

# PATH
export PATH=$HOME/.local/bin:$HOME/.nimble/bin:${PATH}

# Systemd update user path (Enables me to use my custom commands from systemd run services/scripts)
command-exists systemctl &&
	systemctl --user import-environment PATH

for f in ~/.shared_profile.d/*; do
	. source-if-exists "$f"
done
